{
  "extends": "../../tsconfig.base.json",
  "compilerOptions": {
    "jsx": "preserve",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./*"
      ],
      "@suishield/receipt": [
        "../../packages/receipt/src/index.ts"
      ],
      "@suishield/scanner": [
        "../../packages/scanner/src/index.ts"
      ],
      "@suishield/ui": [
        "../../packages/ui/src/index.ts"
      ],
      "@suishield/ui/*": [
        "../../packages/ui/src/*"
      ]
    },
    "plugins": [
      {
        "name": "next"
      }
    ],
    "allowJs": false,
    "incremental": true,
    "isolatedModules": true,
    "types": [
      "node"
    ],
    "noEmit": true
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}
